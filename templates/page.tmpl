<script type="text/javascript">
	$(document).ready(function () {
		jQuery.getFeed({
			url: '/news/index.rss',
			success: function(feed) {
				var html = '<ul>';
				for (var i = 0; i < feed.items.length && i < 3; i++) {
					var item = feed.items[i];
					html += '<li>' +
						'<a href="' + item.link + '">' +
						item.title +
						'</a>' +
						'</li>';
				}
				html += '</ul>';
				$('#latest_news').append(html);
			}
		});
	});
</script>
